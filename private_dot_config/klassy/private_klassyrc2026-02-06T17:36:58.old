[ButtonSizing]
FullHeightButtonWidthMarginLeft=2
FullHeightButtonWidthMarginRight=1

[Global]
LookAndFeelSet=org.kde.breezedark.desktop

[Style]
ButtonGradient=true
FrameCornerRadius=FCR_Custom
FrameCustomCornerRadius=5
MenuOpacity=75
SplitterProxyEnabled=true
TabBarDrawCenteredTabs=true

[TitleBarOpacity]
ActiveTitleBarOpacity=70
InactiveTitleBarOpacity=90

[TitleBarSpacing]
PercentMaximizedTopBottomMargins=77
TitleBarBottomMargin=0
TitleBarTopMargin=0

[Windeco]
BoldButtonIcons=BoldIconsBold
ButtonIconStyle=StyleOxygen
DrawBackgroundGradient=true
IconSize=IconVerySmall
SystemIconSize=SystemIcon18

[WindowOutlineStyle]
LockThinWindowOutlineCustomColorActiveInactive=false
ThinWindowOutlineCustomColorActive=0,255,0
ThinWindowOutlineStyleActive=WindowOutlineCustomWithContrast
ThinWindowOutlineStyleInactive=WindowOutlineCustomWithContrast
ThinWindowOutlineThickness=2.75
WindowOutlineAccentColorOpacityActive=63
WindowOutlineCustomWithContrastOpacityActive=50
WindowOutlineCustomWithContrastOpacityInactive=85
